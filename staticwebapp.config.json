{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "*.{png,jpg,gif,ico,svg}"
    ]
  },
  "routes": [
    {
      "route": "/api/*",
      "methods": [
        "GET",
        "POST"
      ],
      "rewrite": "/api/*"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}

