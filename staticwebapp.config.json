{
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "/api/*"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*.{js,css,svg,png,jpg,gif}", "/vite.svg"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}