{
  "routes": [
    {
      "route": "/api/*",
      "methods": [
        "GET",
        "POST"
      ],
      "rewrite": "/api/*"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/wallets/*",
      "*.png",
      "*.jpg",
      "*.svg"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "platform": {
    "apiRuntime": "node:18"
  }
}

